<%- include('./partials/header.ejs') %>     
    <section class="container mt-4 text-white text-center">
      <div class="row">
        <div class="col-md-3">
          <div class="card shadow">
            <div class="card-body">
              <h2>Patients</h2><hr>
              <h3><%= users.length %></h3>
            </div>
          </div>
        </div>
        <div class="col-md-3">
          <div class="card shadow">
            <div class="card-body">
              <h2>Doctors</h2><hr>
              <h3><%= doctors.length %></h3>
            </div>
          </div>
        </div>
        <div class="col-md-3">
          <div class="card shadow">
            <div class="card-body">
              <h2>Cases</h2><hr>
              <h3><%= 4 + 4 %></h3>
            </div>
          </div>
        </div>
        <div class="col-md-3">
          <div class="card shadow">
            <div class="card-body">
              <h2>Appointments</h2><hr>
              <h3><%= appointments.length %></h3>
            </div>
          </div>
        </div>
      </div>
    </section>


  </body>
  <script src="\bootstrap-5\js\Jquery.js"></script>
  <script src="\bootstrap-5\js\bootstrap.min.js"></script>
  </html>